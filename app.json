{
  "expo": {
    "name": "GoalMe",
    "slug": "GoalMe",
    "version": "1.0.0",
    "sdkVersion": "45.0.0",
    "orientation": "portrait",
    "icon": "./assets/app_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/goalme_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#d5f4ff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.goalme.goalme",
      "supportsTablet": true
    },
    "android": {
      "package": "com.goalme.goalme",
      "adaptiveIcon": {
        "foregroundImage": "./assets/app_icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
